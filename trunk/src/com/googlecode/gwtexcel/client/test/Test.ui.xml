<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:FlexTable ui:field="flexTable"/>
			<g:Grid ui:field="grid">
				<g:row>
					<g:customCell>
						<g:Label text="Grid 1"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Grid 2"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Grid 3"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Grid 4"/>
					</g:customCell>
				</g:row>
			</g:Grid>
			<g:Button ui:field="button1" text="FlexTable"/>
			<g:Button ui:field="button2" text="Grid"/>
		</g:VerticalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 